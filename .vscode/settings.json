{
  "editor.formatOnSave": true,
  "extensions.showRecommendationsOnlyOnDemand": false,
  "prettier.singleQuote": true,
  "prettier.semi": true,
  "prettier.printWidth": 120,
  "prettier.trailingComma": "all",
  "tslint.autoFixOnSave": true,
  "files.watcherExclude": {
    "**/dist/**": true,
    "**/coverage/**": true
  },
  "cSpell.words": [
    "Cognito",
    "amazonaws",
    "api",
    "api's",
    "apigateway",
    "cloudformation",
    "deconfig",
    "deconfigure",
    "eofline",
    "forin",
    "inversify",
    "nospace",
    "quotemark",
    "s",
    "submodule"
  ]
}
